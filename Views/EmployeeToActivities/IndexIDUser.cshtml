@model IEnumerable<PracticeActivityTraking.EmployeeToActivity>
@{
    ViewBag.Title = "Участники активности";
}
@{ Layout = "~/Views/Shared/_Layout.cshtml"; }

<h2>Участники активности</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee.Surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Employee.Patronymic)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.Surname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.Patronymic)
            </td>
        </tr>
    }
   


</table>
<p>
    @Html.ActionLink("Вернуться к списку активностей", "IndexUser", "Activities")
</p>
